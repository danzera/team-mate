{
  "extends": ["eslint:recommended", "plugin:angular/johnpapa"],
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "angular": true,
    "moment": true
  },
  "parserOptions": { 
    "ecmaVersion": 6
  },
  "rules": {
    "no-unused-vars": "warn",
    "quotes": [2, "single"],
    "semi": ["warn", "always"]
  }
}